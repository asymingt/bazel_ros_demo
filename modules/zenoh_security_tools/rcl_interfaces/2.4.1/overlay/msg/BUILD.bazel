# Imports

load("@rosidl_cmake//:rules.bzl", "ros_interface")

package(default_visibility = ["//visibility:public"])

ros_interface(
    name = "FloatingPointRange",
    src = "FloatingPointRange.msg",
)

ros_interface(
    name = "IntegerRange",
    src = "IntegerRange.msg",
)

ros_interface(
    name = "ListParametersResult",
    src = "ListParametersResult.msg",
)

ros_interface(
    name = "Log",
    src = "Log.msg",
    deps = [
        "@builtin_interfaces//msg:Time",
    ],
)

ros_interface(
    name = "LoggerLevel",
    src = "LoggerLevel.msg",
)

ros_interface(
    name = "Parameter",
    src = "Parameter.msg",
    deps = [
        ":ParameterValue",
    ],
)

ros_interface(
    name = "ParameterDescriptor",
    src = "ParameterDescriptor.msg",
    deps = [
        ":FloatingPointRange",
        ":IntegerRange",
    ],
)

ros_interface(
    name = "ParameterEvent",
    src = "ParameterEvent.msg",
    deps = [
        ":Parameter",
        "@builtin_interfaces//msg:Time",
    ],
)

ros_interface(
    name = "ParameterEventDescriptors",
    src = "ParameterEventDescriptors.msg",
    deps = [
        ":ParameterDescriptor",
    ],
)

ros_interface(
    name = "ParameterType",
    src = "ParameterType.msg",
)

ros_interface(
    name = "ParameterValue",
    src = "ParameterValue.msg",
)

ros_interface(
    name = "SetLoggerLevelsResult",
    src = "SetLoggerLevelsResult.msg",
)

ros_interface(
    name = "SetParametersResult",
    src = "SetParametersResult.msg",
)
